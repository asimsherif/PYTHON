      "cell_type": "code",
      "source": [
        "#arithmeticeval\n",
        "def add(x,y):\n",
        "  return x+y\n",
        "def multiply(x,y):\n",
        "  return x*y\n",
        "def evaluate(f,x,y):\n",
        "  return f(x,y)\n",
        "def main():\n",
        "  print(add(2,3))\n",
        "  print(multiply(4,3))\n",
        "  print(evaluate(add,5,3))\n",
        "  print(evaluate(multiply,2,3))\n",
        "main()"
      ],
